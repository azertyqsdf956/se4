<VirtualHost *:80>

	DocumentRoot /var/www/sambaedu/ipxe/
    Alias /diconf/ /var/www/sambaedu/ipxe/diconf/	
    <Directory /var/www/sambaedu/ipxe>
		Options Indexes FollowSymLinks MultiViews
		AllowOverride None
		Order allow,deny
		allow from all
	</Directory>
	<FilesMatch "diconf/*.tar.gz$">
        AllowOverride None
        Order deny,allow
        Deny from all
        Allow from ###_SE4AD_IP_###
        Allow from ###_SE4FS_IP_###
    </FilesMatch>
	ErrorLog /var/log/apache2/error.log
</VirtualHost>
